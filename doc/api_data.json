ersion": "0.0.0",    "filename": "./agent.py",    "groupTitle": "Sites"  },  {    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "optional": false,            "field": "varname1",            "description": "<p>No type.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "varname2",            "description": "<p>With type.</p> "          }        ]      }    },    "type": "",    "url": "",    "version": "0.0.0",    "filename": "./doc/main.js",    "group": "_Users_fservais_Projects_Nginx_Agent_doc_main_js",    "groupTitle": "_Users_fservais_Projects_Nginx_Agent_doc_main_js",    "name": ""  }]